{
  "name": "React-Webpack2-Starter-Kit",
  "description": "Quiq Webchat",
  "version": "1.0.4",
  "author": "Quiq",
  "license": "ISC",
  "private": true,
  "keywords": [
    "react",
    "es6",
    "hot",
    "babel",
    "webpack",
    "webpack-dev-middleware",
    "webpack-hot-middleware",
    "boilerplate",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/quiq/react-webpack2-starter-kit"
  },
  "scripts": {
    "prestart": "yarn run rimraf dist",
    "start": "webpack-dashboard -c cyan -- node dev-server.js",
    "prebuild": "yarn run rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.production.js --progress",
    "postbuild": "mv dist/index.html dist/index.js",
    "jest": "jest --watch --env=jsdom",
    "flow": "flow",
    "lint": "node ./node_modules/eslint/bin/eslint.js app/",
    "test": "flow && jest --env=jsdom && node ./node_modules/eslint/bin/eslint.js app/ --quiet"
  },
  "jest": {
    "testRegex": "Spec\\.js$",
    "modulePaths": [
      "app/components",
      "app",
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(core-ui)/).*/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|jpg|png|mp3|wav|woff|aff|dic|svg)$": "<rootDir>/EmptyModule.js"
    },
    "snapshotSerializers": [
      "<rootDir>/../node_modules/enzyme-to-json/serializer"
    ],
    "globals": {
      "QUIQ": {
        "CONTACT_POINT": "Bob",
        "HOST": "https://bob.dev.centricient.corp",
        "COLOR": "#333",
        "HEADER_TEXT": "TOOL TIME"
      },
      "__DEV__": true
    },
    "rootDir": "app"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-es2015-native-modules": "6.9.4",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "empty": "0.10.1",
    "enzyme": "2.4.1",
    "enzyme-to-json": "1.4.5",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-flowtype": "2.11.4",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "0.9.0",
    "flow-bin": "0.43.0",
    "html-webpack-plugin": "2.28.0",
    "http-server": "0.9.0",
    "jest": "19.0.1",
    "jsdom": "9.4.5",
    "json-loader": "0.5.4",
    "namespace-css-loader": "0.0.2",
    "node-sass": "4.5.2",
    "postcss-loader": "0.11.1",
    "react-addons-test-utils": "15.4.2",
    "react-test-renderer": "15.4.2",
    "redbox-react": "1.3.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "style-loader": "0.13.1",
    "webpack": "2.1.0-beta.21",
    "webpack-dashboard": "0.3.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-merge": "0.14.1"
  },
  "dependencies": {
    "atmosphere.js": "https://github.com/Quiq/atmosphere-js/tarball/cf35c913143cf2c391b6bcb694ae8a47794a1b88",
    "autoprefixer": "6.4.1",
    "babel-runtime": "6.11.6",
    "body-parser": "1.16.0",
    "classnames": "2.2.5",
    "core-ui": "git+ssh://git@gitlab.quiq.sh/development/core-ui.git#0.0.4",
    "cors": "2.8.1",
    "express": "4.14.0",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.15.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-hot-loader": "3.0.0-beta.2",
    "react-intl": "2.2.3",
    "react-linkify": "0.2.0",
    "react-textarea-autosize": "4.0.5"
  }
}
