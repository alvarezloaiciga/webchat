{
  "name": "Webchat",
  "description": "Quiq Webchat",
  "version": "1.1.80",
  "author": "Quiq",
  "license": "ISC",
  "private": true,
  "keywords": [
    "webchat"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.quiq.sh/development/webchat"
  },
  "scripts": {
    "prestart": "yarn run rimraf dist",
    "start": "webpack-dashboard -c cyan -p 9840 -- node dev-server.js",
    "prevmstart": "yarn run rimraf dist",
    "vmstart": "./node_modules/.bin/nf start",
    "prebuild": "yarn run rimraf dist temp_docker",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.production.js --progress",
    "postbuild": ". config/postbuild.sh",
    "jest": "jest --watch --env=jsdom",
    "flow": "flow",
    "lint": "node ./node_modules/eslint/bin/eslint.js app/ SDK/ Common/",
    "test": "flow && jest --env=jsdom && node ./node_modules/eslint/bin/eslint.js app/ --quiet",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "app/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "app/**/*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock",
      "./jest.setup.js"
    ],
    "notify": true,
    "modulePaths": [
      "./",
      "app/components",
      "app",
      "SDK/src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|jpg|png|mp3|wav|woff|aff|dic|svg)$": "<rootDir>/app/EmptyModule.js"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "globals": {
      "quiqOptions": {
        "contactPoint": "Bob",
        "host": "https://bob.dev.centricient.corp",
        "color": "#333",
        "headerText": "TOOL TIME",
        "debug": false,
        "welcomeForm": null
      },
      "Modernizr": {
        "flexbox": true,
        "svg": true,
        "flexwrap": true,
        "svgfilters": true,
        "inlinesvg": true
      },
      "__DEV__": true,
      "__VERSION__": "1.0.0"
    },
    "rootDir": "./",
    "roots": [
      "app",
      "Common"
    ]
  },
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "21.0.2",
    "babel-loader": "6.4.1",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-es2015-native-modules": "6.9.4",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "body-parser": "1.16.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "4.0.1",
    "cors": "2.8.1",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "ejs": "^2.5.7",
    "empty": "0.10.1",
    "enzyme": "2.8.2",
    "enzyme-to-json": "1.4.5",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-flowtype": "2.11.4",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "0.9.0",
    "flow-bin": "0.54.1",
    "foreman": "2.0.0",
    "html-webpack-plugin": "2.28.0",
    "http-server": "0.9.0",
    "husky": "0.13.3",
    "jest": "21.0.2",
    "jest-localstorage-mock": "1.2.0",
    "jsdom": "9.4.5",
    "json-loader": "0.5.4",
    "lint-staged": "3.4.0",
    "morgan": "1.8.2",
    "namespace-css-loader": "0.0.2",
    "ngrok": "2.2.9",
    "node-sass": "4.5.2",
    "postcss-loader": "0.11.1",
    "prettier": "1.6.1",
    "react-addons-test-utils": "15.6.0",
    "react-hot-loader": "3.0.0-beta.2",
    "react-test-renderer": "^15.5.4",
    "redbox-react": "1.3.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "style-loader": "0.13.1",
    "susy": "2.2.12",
    "webpack": "2.1.0-beta.21",
    "webpack-dashboard": "0.3.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-merge": "0.14.1"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "babel-runtime": "6.11.6",
    "classnames": "2.2.5",
    "draft-js": "^0.10.3",
    "draft-js-plugins-editor": "beta",
    "emoji-mart": "2.0.1",
    "emoji-regex": "6.5.1",
    "file-saver": "1.3.3",
    "intl": "1.2.5",
    "lodash": "4.17.4",
    "post-robot": "Quiq/post-robot#394568a87d63fd24b9289bb2a1f0223b9bd18752",
    "preact": "8.2.5",
    "quiq-chat": "1.20.1",
    "react": "15.6.0",
    "react-addons-update": "15.6.0",
    "react-click-outside-component": "1.1.1",
    "react-dom": "15.6.0",
    "react-easy-emoji": "1.1.0",
    "react-intl": "2.3.0",
    "react-linkify": "0.2.1",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-textarea-autosize": "5.1.0",
    "redux": "3.7.2",
    "redux-devtools": "3.4.0",
    "redux-devtools-log-monitor": "1.3.0",
    "redux-store-watch": "0.0.11"
  }
}
