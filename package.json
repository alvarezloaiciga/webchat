{
  "name": "Webchat",
  "description": "Quiq Webchat",
  "version": "1.0.92",
  "author": "Quiq",
  "license": "ISC",
  "private": true,
  "keywords": [
    "webchat"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.quiq.sh/development/webchat"
  },
  "scripts": {
    "prestart": "yarn run rimraf dist",
    "start": "webpack-dashboard -c cyan -- node dev-server.js",
    "prevmstart": "yarn run rimraf dist",
    "vmstart": "./node_modules/.bin/nf start",
    "prebuild": "yarn run rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.production.js --progress",
    "postbuild": "mv dist/index.html dist/index.js",
    "jest": "jest --watch --env=jsdom",
    "flow": "flow",
    "lint": "node ./node_modules/eslint/bin/eslint.js app/",
    "test": "flow && jest --env=jsdom && node ./node_modules/eslint/bin/eslint.js app/ --quiet",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "app/**/*.js": [
      "prettier --single-quote --trailing-comma all --parser flow --no-bracket-spacing --print-width 100 --write",
      "git add"
    ],
    "app/**/*.scss": [
      "prettier --single-quote --trailing-comma all --parser flow --no-bracket-spacing --print-width 100 --write",
      "git add"
    ]
  },
  "jest": {
    "modulePaths": [
      "app/components",
      "app",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|jpg|png|mp3|wav|woff|aff|dic|svg)$": "<rootDir>/EmptyModule.js"
    },
    "snapshotSerializers": [
      "<rootDir>/../node_modules/enzyme-to-json/serializer"
    ],
    "globals": {
      "QUIQ": {
        "CONTACT_POINT": "Bob",
        "HOST": "https://bob.dev.centricient.corp",
        "COLOR": "#333",
        "HEADER_TEXT": "TOOL TIME",
        "DEBUG": false,
        "WELCOME_FORM": null
      },
      "Modernizr": {
        "flexbox": true,
        "svg": true,
        "flexwrap": true,
        "svgfilters": true,
        "inlinesvg": true
      },
      "__DEV__": true
    },
    "rootDir": "app"
  },
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-es2015-native-modules": "6.9.4",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "body-parser": "1.16.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "cookie-parser": "1.4.3",
    "copy-webpack-plugin": "4.0.1",
    "cors": "2.8.1",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "empty": "0.10.1",
    "enzyme": "2.8.2",
    "enzyme-to-json": "1.4.5",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-flowtype": "2.11.4",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "0.9.0",
    "flow-bin": "0.45.0",
    "foreman": "2.0.0",
    "html-webpack-plugin": "2.28.0",
    "http-server": "0.9.0",
    "husky": "0.13.3",
    "jest": "19.0.1",
    "jsdom": "9.4.5",
    "json-loader": "0.5.4",
    "lint-staged": "3.4.0",
    "morgan": "1.8.2",
    "namespace-css-loader": "0.0.2",
    "ngrok": "2.2.9",
    "node-sass": "4.5.2",
    "postcss-loader": "0.11.1",
    "prettier": "1.5.2",
    "react-addons-test-utils": "15.6.0",
    "react-hot-loader": "3.0.0-beta.2",
    "react-test-renderer": "^15.5.4",
    "redbox-react": "1.3.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "style-loader": "0.13.1",
    "susy": "2.2.12",
    "webpack": "2.1.0-beta.21",
    "webpack-dashboard": "0.3.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-merge": "0.14.1"
  },
  "dependencies": {
    "atmosphere.js": "https://github.com/Quiq/atmosphere-js/tarball/cf35c913143cf2c391b6bcb694ae8a47794a1b88",
    "babel-polyfill": "6.23.0",
    "babel-runtime": "6.11.6",
    "classnames": "2.2.5",
    "intl": "1.2.5",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.1.4",
    "lodash": "4.15.0",
    "qs": "6.4.0",
    "quiq-chat": "1.1.3",
    "react": "15.6.0",
    "react-dom": "15.6.0",
    "react-intl": "2.3.0",
    "react-linkify": "0.2.1",
    "react-router-dom": "4.1.1",
    "react-textarea-autosize": "5.0.6"
  }
}
